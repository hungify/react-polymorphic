{
  "name": "vite-react-ts-starter",
  "private": true,
  "version": "0.0.2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "tsc --noEmit -p tsconfig.json && biome lint --diagnostic-level=error --no-errors-on-unmatched src/**/*.{js,ts}",
    "lint:fix": "biome check --apply-unsafe src/**/*.{js,ts}",
    "lint:staged": "biome check --error-on-warnings --no-errors-on-unmatched --staged src/**/*.{js,ts}",
    "format": "biome format src/**/*.{js,ts}"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.2.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "framer-motion": "^11.16.4",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "license": "MIT"
}
